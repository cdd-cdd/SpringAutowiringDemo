<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd"
        default-autowire="byType">

	<bean id="logger" class="com.able.beans.Logger" ></bean>	
	<bean id="consoleWriter" name="consolewriter" class="com.able.beans.ConsoleWriter" ></bean>	
	<bean id="fileWriter" class="com.able.beans.FileWriter" primary="true" ></bean>
	<bean id="trick" class="com.able.beans.FileWriter">
</bean>


</beans>